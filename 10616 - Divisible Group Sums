#include <bits/stdc++.h>
using namespace std;
int N,M, D;
int valor[10000];
int cont;
int PD[1000][1000];
int mochila(int item, int soma, int conta)
{
	if(item == N ) return 0;
	/*if(PD[item][soma] != -1)
		return PD[item][soma];*/
	if(conta == M )
	{
		if(soma%D == 0)
		{
			return 1;			
		}
		return 0;		
	}
	else  
	PD[item][soma] = mochila(item+1, valor[item] + soma, conta+1) +  mochila(item+1, soma, conta);
	return PD[item][soma];
}
int main()
{
	int Q;
	while(scanf("%d%d", &N, &Q), N || Q)
	{
		for (int i = 0; i < N; i++)
		{
			scanf("%d", &valor[i]);
		}
		cont = 0;
		for (int i = 0; i < Q; i++)
		{
			scanf("%d%d", &D, &M);
			printf("%d\n", mochila(0,0,0));
		}

	}
	
}
