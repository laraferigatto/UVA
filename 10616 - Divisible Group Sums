#include <bits/stdc++.h>
using namespace std;
int N,M, D;
int valor[210];
int cont;
int PD[210][10000];
int mochila(int item, int soma, int conta)
{
	if(conta == M )
	{
		if(soma%D == 0)
			return 1;
		return 0;
	}
	
	if(item == N )
	{		
		 return 0;
	}
	if(PD[item][soma] != -1) return PD[item][soma];
	
	
	return PD[item][soma] = mochila(item+1, valor[item] + soma, conta+1) +  mochila(item+1, soma, conta);
	
	
	

}
int main()
{
	int Q;
	cont = 0;
	while(scanf("%d%d", &N, &Q), N || Q)
	{
		cont++;
		for (int i = 0; i < N; i++)
		{
			scanf("%d", &valor[i]);
		}
	
		for (int i = 0; i < Q; i++)
		{
			memset(PD, -1, sizeof PD);
			scanf("%d%d", &D, &M);
			if(i == 0)
				printf("SET %d:\n", cont);
			printf("QUERY %d: %d\n", i+1, mochila(0,0,0));
		}

	}
	
}
